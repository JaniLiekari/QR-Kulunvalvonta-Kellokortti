<html>
	<head>
		<meta charset="utf-8">
		<link rel="stylesheet" href="http://<%=IP%>:<%=PORT%>/file/css/bootstrap.min.css">
		<link rel="shortcut icon" type="image/vnd.microsoft.icon" href="./img/favicon.ico"/>
		<title>Kulunvalvonta</title>
	</head>
	<script src="http://<%=IP%>:<%=PORT%>/file/js/jquery-3.3.1.js" type="text/javascript"></script>
	<body>

			<!-- SIVUJEN HALLINTA -->

		<% if(user) { %>
			<% include ./partials/menu.html %>
			<% if(page === "main") { %>
					<% include ./partials/main.html %>
			<% } else if(page === "create") { %>
					<% include ./partials/create.html %>
			<% }else if(page === "modify-client") { %> 
					<% include ./partials/modify-client.html %>
			<% }else if(page === "options") { %>
					<% include ./partials/options2.html %>
			<% }else if(page === "statistic"){ %>
					<% include ./partials/statistic.html %>
			<% } else { %>

					<!-- PLUGIN HOOK -->

					<% var hookUp = HookPlugin(plugins, "ROUTE", ""); %>
					<% for(var i = 0; i < hookUp.length; i++){%>
							<%- include(hookUp[i])%>
					<%}%>

					<!-- ----------- -->	

			<%}%>
				<% if(QRLOG == true){ %> 
					<% include ./partials/WEBSOCKET/ws.html %>
				<% } %>
		<%  } else { %>
			<% include ./partials/logsite.html %>
		<% } %>

			<!-- ------------ --->




	</body>


	<script src="http://<%=IP%>:<%=PORT%>/file/js/popper.js" type="text/javascript"></script>
	<script src="http://<%=IP%>:<%=PORT%>/file/js/bootstrap.min.js" type="text/javascript"></script>

</html>
